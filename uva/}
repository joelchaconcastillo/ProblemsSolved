#include <bits/stdc++.h>
#define MAX_N 100
using namespace std;
int N, M;
vector<pair<int, pair<int, int> > > edges;
vector<int> enabled;
vector<int> selected_edges;
void kruskal()
{
}
int main()
{
  int T;
  cin >> T;
  while(T--)
  {
     cin >> N > M ;
     for(int i = 0; i < M ; i++)
     {
	int ai, bi, ci;
       cin >> ai >>bi>> ci;
       edges.push_back(make_pair(ci-1, make_pair(bi-1, ai-1)));
     } 

     sort(edges.begin(), edges.end());
     

  }
  return 0;
}

